{
  "name": "wog",
  "version": "1.0.0-rc.2",
  "description": "A web interface for viewing logs.",
  "main": "backend/server.js",
  "scripts": {
    "sass": "node sass",
    "dev": "DEBUG=true node backend/server",
    "compile:sass": "parcel build frontend/src/sass/base.sass --no-source-maps -d frontend/static/css --out-file style.min.css",
    "compile:js": "parcel build frontend/src/js/logi*.js --no-source-maps -d frontend/static/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelrindle/wog.git"
  },
  "keywords": [
    "web",
    "interface",
    "logs"
  ],
  "author": "Axel Rindle <axel.rindle@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axelrindle/wog/issues"
  },
  "homepage": "https://github.com/axelrindle/wog#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chokidar": "^2.0.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "glob-all": "^3.1.0",
    "is-file": "^1.0.0",
    "moment": "^2.22.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pretty-bytes": "^5.1.0",
    "pug": "^2.0.3",
    "signale": "^1.3.0"
  },
  "devDependencies": {
    "browserslist": "^4.1.1",
    "bulma": "^0.7.1",
    "cssnano": "^4.1.4",
    "cssnano-preset-advanced": "^4.0.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.10.1",
    "write": "^1.0.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "fsevents": "^1.2.4",
    "utf-8-validate": "^5.0.1"
