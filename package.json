{
  "name": "wog",
  "version": "1.0.0-rc.2",
  "description": "A web interface for viewing logs.",
  "main": "backend/main.js",
  "scripts": {
    "start": "node backend/main",
    "dev": "DEBUG=true node backend/main",
    "compile:sass": "parcel build frontend/src/sass/style.sass --no-source-maps -d frontend/static/css --out-file style.min.css",
    "compile:js": "parcel build frontend/src/js/logi*.js --no-source-maps -d frontend/static/js",
    "watch:js": "parcel watch frontend/src/js/logi*.js --no-source-maps -d frontend/static/js --no-hmr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelrindle/wog.git"
  },
  "keywords": [
    "web",
    "interface",
    "logs"
  ],
  "author": "Axel Rindle <axel.rindle@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axelrindle/wog/issues"
  },
  "homepage": "https://github.com/axelrindle/wog#readme",
  "engines": {
    "node": ">=10.15.3"
  },
  "dependencies": {
    "@axelrindle/async-for-each": "^1.0.0",
    "body-parser": "^1.18.3",
    "chokidar": "^2.0.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "glob-all": "^3.1.0",
    "helmet": "^3.15.0",
    "is-directory": "^0.3.1",
    "is-file": "^1.0.0",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "nanoid": "^2.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pretty-bytes": "^5.1.0",
    "pug": "^2.0.3",
    "signale": "^1.3.0",
    "vue-is-mounted": "^1.0.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "bulma": "^0.7.1",
    "eslint": "^5.16.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.15.2",
    "vue-template-compiler": "^2.6.10"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "fsevents": "^1.2.4",
    "utf-8-validate": "^5.0.1"
  }
}
