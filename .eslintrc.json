{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "DEBUG": "readonly",
        "ROOT_DIRECTORY": "readonly",
        "accounts": true,
        "config": true,
        "logger": true,
        "adapters": true,
        "storage": true,
        "Vue": true,
        "axios": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "no-case-declarations": "off"
    }
}
