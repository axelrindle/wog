{
  "dontLoad": [
    "node_modules/**",
    ".github/**",
    ".cache/**"
  ],
  "libs": [
    "browser",
    "jquery"
  ],
  "plugins": {
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "commonjs": {}
  },
  "loadEagerly": [
    "backend/*.js",
    "frontend/**/*.vue"
  ]
}