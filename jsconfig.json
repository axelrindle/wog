{
  "compilerOptions": {
    "module": "commonJS",
    "target": "es6",
    "baseUrl": "./backend/",
    "typeRoots": [
      "./node_modules/@wogjs/types/"
    ],
    "checkJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": [
    "backend/**/*.js",
    "config/*.js"
  ],
  "typeAcquisition": {
    "enable": true,
    "include": [
      "jquery"
    ]
  }
}
